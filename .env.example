# Environment name (matches key in config.json environments)
CHAIN_ENV=local

# Founder wallet (used when privateKeySource: "env" in config.json)
FOUNDER_WALLET_PK=0x_your_founder_wallet_private_key
COSMOS_FOUNDER_WALLET_PK=your_cosmos_founder_wallet_private_key

# Load test wallets (used when walletEnvPrefix: "LOAD_WALLET_PK" in load-stress.config.json)
LOAD_WALLET_PK_1=0x_your_load_test_wallet_1_private_key
LOAD_WALLET_PK_2=0x_your_load_test_wallet_2_private_key
LOAD_WALLET_PK_3=0x_your_load_test_wallet_3_private_key

# Contract testing (optional)
CONTRACT_ADDRESS=test_contract_address
TOPIC=test_topic

# SSH key for remote node operations (optional, base64 encoded)
# SSH_KEY=base64_encoded_private_key
